local lc = game:GetService("Players").LocalPlayer 
local group = 17317757 
local grouplink = "17317757"
local creds = "剑客"

if lc:IsInGroup(group) then 
      print("欢迎使用剑客")
else
      lc:kick("🍙请加群组搜索剑客🍙") 
      setclipboard(grouplink)
end

print(creds)

